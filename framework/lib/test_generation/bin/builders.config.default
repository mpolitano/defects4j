#
# Additional configuration options for Randoop.
#
# All commented lines are ignored.
#
--flaky-test-behavior=output
--string-maxlen=5000
--forbid-null=false
--null-ratio=0.1
--only-test-public-members=false
--omitmethods=HashCodeAndEqualsSafeSet.of
--use-builder-inputs-in-nonbuilder-seq-prob=0.7
--select-builder-prob=0.1
--disable-contracts=true
--generic-types-heuristic=false
--select-builder-constructor-prob=0.5
--select-non-builder-constructor-prob=0.3
